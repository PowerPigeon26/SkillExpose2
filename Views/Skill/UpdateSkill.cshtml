@model Skill

<h2>Update @Model.Name</h2>

@using (Html.BeginForm("UpdateSkillToDatabase", "Skill", "Post"))
{
    <input asp-for="ID" type="hidden" value="@Model.ID" />

    <label asp-for="Name" class="control-label">Name</label>
    <input type="text" name="Name" class="form-control" value="@Model.Name" />

    <label asp-for="GameID" class="control-label">GameID</label>
    <input type="text" name="GameID" class="form-control" value="@Model.GameID" />

    <label asp-for="Game" class="control-label">Game</label>
    <input type="text" name="Game" class="form-control" value="@Model.Game" />

    <label asp-for="Type" class="control-label">Type</label>
    <input type="text" name="Type" class="form-control" value="@Model.Type" />

    <label asp-for="InGameDescription" class="control-label">InGameDescription</label>
    <input type="text" name="InGameDescription" class="form-control" value="@Model.InGameDescription" />

    <label asp-for="AdditionalDescription" class="control-label">AdditionalDescription</label>
    <input type="text" name="AdditionalDescription" class="form-control" value="@Model.AdditionalDescription" />

    <label asp-for="Notes" class="control-label">Notes</label>
    <input type="text" name="Notes" class="form-control" value="@Model.Notes" />

    <label asp-for="YTVideoName" class="control-label">YTVideoName</label>
    <input type="text" name="YTVideoName" class="form-control" value="@Model.YTVideoName" />

    <label asp-for="YTCode" class="control-label">YTCode</label>
    <input type="text" name="YTCode" class="form-control" value="@Model.YTCode" />

    <label asp-for="TSStart" class="control-label">TSStart</label>
    <input type="text" name="TSStart" class="form-control" value="@Model.TSStart" />

    <label asp-for="TSEnd" class="control-label">TSEnd</label>
    <input type="text" name="TSEnd" class="form-control" value="@Model.TSEnd" />

    <input type="submit" value="Update Skill" class="btn btn-primary" />
}